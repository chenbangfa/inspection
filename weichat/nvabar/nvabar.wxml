<view class='nav-wrap' style='height: {{height}}px;'>
  <view class='nav-title' style='padding-top:{{navTop}}px;'>{{navbarData.title}}</view>
  <view style='display: flex; justify-content: space-around;flex-direction: column'>
    <view class='nav-capsule' style='padding-top:{{navTop}}px;' wx:if='{{navbarData.showCapsule}}'>
      <view bindtap='_backhome'>
        <image src='/image/home.png' mode='aspectFill' class='back-pre'></image>
      </view>
    <view class='navbar-v-line' wx:if='{{!share}}'></view>
      <view bindtap='_navback'>
        <image src='/image/fanhui.png' mode='aspectFill' class='back-home'></image>
      </view>
    </view>
  </view>
</view>